{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "postinstall": "npx update-browserslist-db@latest --yes",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,css,md,html,json,yml}\" --ignore-unknown",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,css,md,html,json,yml}\" --ignore-unknown",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "framer-motion": "^11.2.4",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.3",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.4.7",
    "@testing-library/react": "^14.2.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.35",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md,yml,html}": "prettier --write"
  },
  "author": "SmarterLogicWeb <[email]>",
  "homepage": "https://smarterlogicweb.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Soofmax/ibrX2.git"
  },
  "bugs": {
    "url": "https://github.com/Soofmax/ibrX2/issues",
    "email": "[email]"
  },
  "license": "MIT"
}
